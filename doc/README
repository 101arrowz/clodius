### Process the real data files

python scripts/tile_bigWig.py 

for file in ~/data/encode/hg19/*.bigWig;
    do
    echo $file;
    python scripts/tile_bigWig.py $file;
done;

for file in ~/data/encode/hg19/*.hitile; do aws s3 cp $file s3://pkerp/public/hg-server/data/; done;
for file in $(ls ~/Downloads/*10kb*multires*); do aws s3 cp $file s3://pkerp/public/hg-server/data/; done;
aws s3 cp ~/data/genbank-data/human/geneAnnotationsExonUnions.bed s3://pkerp/public/data/hg-server/preprocessed/
aws s3 cp  ~/data/genbank-data/human/gene-annotations-hg19.db s3://pkerp/public/hg-server/data/
